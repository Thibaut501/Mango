@{
    var success = TempData["success"]?.ToString();
    var successCap = TempData["Success"]?.ToString();
    var error = TempData["error"]?.ToString();
    var errorCap = TempData["Error"]?.ToString();
}
@if (!string.IsNullOrWhiteSpace(success))
{
    <div data-temp-success="@success" hidden></div>
}
@if (!string.IsNullOrWhiteSpace(successCap))
{
    <div data-temp-success="@successCap" hidden></div>
}
@if (!string.IsNullOrWhiteSpace(error))
{
    <div data-temp-error="@error" hidden></div>
}
@if (!string.IsNullOrWhiteSpace(errorCap))
{
    <div data-temp-error="@errorCap" hidden></div>
}